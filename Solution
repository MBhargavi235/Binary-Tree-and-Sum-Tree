node=26
left_node= 10
right_node = 3
left_left_node = 4
left_right_node= 6
right_right_node = 3
def isBinarySuumTree(node,left_node,right_node,left_left_node,left_right_node,right_right_node):
    if node==(left_node+right_node+left_left_node+left_right_node+right_right_node):
        return 1
    else:
        return 0 
print(isBinarySuumTree(node,left_node,right_node,left_left_node,left_right_node,right_right_node))
